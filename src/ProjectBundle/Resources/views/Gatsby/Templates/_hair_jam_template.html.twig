{% if (products|length >0) %}
{% for product in products %}
            <!-- #spiky-edge start -->
        <section id="{{product.getTitle|raw|slug}}" class="item-wrapper">
            {% if product.imageInnerVideo and product.video %}
            <div class="item-main section-block">
                <h2>{{product.getTitle|raw}}</h2>
                <span class="pic">
                    {% if product.imageInnerVideo %}
                        <img src="{{product.imageInnerVideo}}" alt="ムービングラバー スパイキーエッジ" />
                    {% endif %}
                </span>
                <!-- <div class="section-inner">
                <h3 class="tit-vertical">TECHNIQUES</h3>
                </div> -->
                <div class="movie" id="{{product.getTitle|raw|slug}}-techniques">
                    <div class="movie-in">
                        {% if product.video %}
                            <iframe src="https://www.youtube.com/embed/{{product.videoId}}?rel=0&amp;showinfo=0" frameborder="0" allow="autoplay; encrypted-media" allowfullscreen></iframe>
                        {% endif %}
                    </div><!-- .movie-in -->
                </div><!-- .movie -->
                <p class="en">{% if (series|length > 0) %}{{series[0].getTitle|upper}}  {% else %}ไม่พบซีรี่นี้{% endif %}<span>{{product.getTitle|raw}}</span></p>
            </div><!-- .main -->
            {% endif %}
        <section class="section-block grid-column">
            {% if product.productTechNique|length > 0 %}
            <div class="tech-section section-inner">
                <!-- <h3 class="tit-vertical techniques-pc-title">TECHNIQUES</h3> -->
                <h3 class="tit-vertical">TECHNIQUES</h3>
                <div class="section-content">
                    <div class="step">
                        <div class="slick-wrapper">
                            {% for technique in product.productTechNique %}
                            <div class="slick-content">
                                <div class="step-img">
                                    <img data-lazy="{{technique.Image}}" alt="" />
                                </div>
                                <dl>
                                    <dt>
                                        <div class="no"><span>STEP</span>{{loop.index}}</div>
                                    </dt>
                                    <dd>
                                        <p>{{technique.getTitle}}</p>
                                    </dd>
                                </dl>
                            </div>{# .slick-content #}
                            {% endfor %}
                        </div>{# .slick-wrapper #}
                    </div>{# .step #}
                </div><!-- .section-content -->
            </div><!-- .tech-section -->
            {% endif %}
            {% if product.productImages|length > 0 %}
            <div class="style-section section-inner">
                <h3 class="tit-vertical">STYLE</h3>

                <div class="section-content">
                    <div class="style">
                        <div class="slick-wrapper">
                          {% for style_img in product.productImages %}
                            <div class="slick-content">
                                <div class="style-img">
                                    <img data-lazy="{{style_img.image}}" alt="" />
                                </div>
                            </div>{# .slick-content #}
                          {% endfor %}
                        </div>{# .slick-wrapper #}
                    </div>{# .style #}
              <!-- <div class="comment">
                <div class="supervised">STYLED BY</div>
                <div class="shop">ROOTS</div>
                <div class="name">Takako Kato</div>
                <p>Spiky Edge is a hard-holding styling product that is easy to blend and apply, creating defined hairstyles effortlessly. It fluently creates bunches and movement. Spiky Edge also holds up your hair if you've raised it around the Mohawk line. You don't even have to use a holding spray.</p>
                <div class="person"><img src="../../../img/spiky_edge_supervised.png" alt="ムービングラバー スパイキーエッジ" /></div>
              </div><!-- .comment -->
                </div><!-- .section-content -->
            </div><!-- .style-section -->
            {% endif %}
            {% if product.imageRadarChart %}
            <div class="chart-section section-inner">
                <h3 class="tit-vertical">RADAR CHART</h3>
                <div class="section-content">
                    <span class="pic">
                        <img src="{{product.imageRadarChart}}" alt="RADAR CHART" /></span>
                        {#<div class="chart-table">
                            <h4 class="tit">LENGTH</h4>
                            <ul class="table">
                                <li class="on"><span>VERY <br class="sp-br">SHORT</span></li>
                                <li class="on"><span>SHORT</span></li>
                                <li><span>MEDIUM <br class="sp-br">SHORT</span></li>
                                <li><span>MEDIUM & <br class="sp-br">LONGER</span></li>
                            </ul>                                             <!--     <h4 class="tit">おすすめスタイル</h4>
                  <ul class="table">
                  <li class="on"><span>立ち上げ系</span></li>
                  <li class="on"><span>毛束系</span></li>
                  <li><span>ふんわり系</span></li>
                  <li><span>まとまり系</span></li>
                </ul>-->
                        </div><!-- .chart-table-wrapper -->#}
                </div><!-- .section-content -->
            </div><!-- .chart-section -->
            {% endif %}
        </section>

        <section class="section-block">
            <div class="more-section section-inner">
                <h4 class="acc-btn tit-vertical">MORE</h4>
                <div class="section-content acc-body">
                    <div class="col2-wrap">
                        <div class="col2-inner">
                            {% if product.getSpecialFeature %}
                                <div class="inner-box">
                                    <h5>SPECIAL FEATURES</h5>
                                    <ul class="disc-list">
                                        {{product.getSpecialFeature|raw}}
                                    </ul>
                                </div>
                            {% endif %}
                            {% if product.getHowToUse %}
                                <div class="inner-box">
                                    <h5>HOW TO USE</h5>
                                    <ul class="disc-list">
                                        {{product.getHowToUse|raw}}
                                    </ul>
                                </div>
                            {% endif %}
                        </div>
                        <div class="col2-inner">
                            {% if product.getNetWeight %}
                                <div class="inner-box">
                                    <h5>NET WEIGHT</h5>
                                    <ul class="disc-list">
                                        {{product.getNetWeight|raw}}
                                    </ul>
                                </div>
                            {% endif %}
                            <!-- <div class="inner-box">
                            <p class="icon"><span>PARABEN FREE </span></p>
                            </div> -->
                        </div>
                    </div>{# .col2-wrap #}
                </div><!-- .section-content -->
            </div><!-- .more-section -->
        </section>

</section>
      <!-- #spiky-edge end -->
      {% endfor %}
{% endif %}

